{
  "name": "create-next-stack",
  "description": "Create Next Stack is a website and CLI tool used to easily set up the boilerplate of new Next.js apps.",
  "version": "0.2.6",
  "author": "Anders KjÃ¦r Damgaard @akd-io",
  "bugs": "https://github.com/akd-io/create-next-stack/issues",
  "homepage": "https://github.com/akd-io/create-next-stack",
  "repository": "akd-io/create-next-stack",
  "license": "MIT",
  "keywords": [
    "react",
    "cli",
    "template",
    "boilerplate",
    "typescript",
    "web",
    "eslint",
    "frontend",
    "nextjs",
    "prettier",
    "starter",
    "starter-kit",
    "scaffolding"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "oclif": {
    "bin": "create-next-stack",
    "commands": "./lib/main/commands",
    "default": "create-next-stack",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "bin": {
    "create-next-stack": "./bin/run"
  },
  "files": [
    "/bin",
    "/lib/main",
    "/prod-assets"
  ],
  "exports": {
    "./package.json": "./package.json"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "prepack": "pnpm build",
    "check-types": "tsc --noEmit",
    "check-types:watch": "tsc --noEmit --watch",
    "build": "rimraf lib && tsc --build",
    "build:watch": "pnpm build --watch",
    "clean": "rimraf lib && pnpm clean-tests-dir",
    "clean-tests-dir": "ts-node src/tests/e2e/clean-tests-dir.ts",
    "test": "pnpm build && pnpm jest-unit && pnpm jest-e2e",
    "jest": "jest",
    "jest-unit": "jest src/main/",
    "jest-unit:ci": "jest --ci src/main/",
    "jest-unit:watch": "pnpm jest-unit --watch",
    "get-e2e-test-files-array": "jest --listTests src/tests/e2e/ | ts-node src/scripts/file-list-to-array.ts",
    "jest-e2e": "jest --runInBand src/tests/e2e/",
    "jest-e2e:watch": "pnpm jest-e2e --watch",
    "jest-e2e:single": "jest --runInBand",
    "jest-e2e:ci": "jest --runInBand --ci",
    "test:manual": "pnpm build && ts-node src/tests/e2e/test-manual.ts --debug",
    "test:cna": "pnpm build && ts-node src/tests/e2e/test-live-cna.ts",
    "test:cns": "pnpm build && ts-node src/tests/e2e/test-live-cns.ts",
    "test:raw": "pnpm build && ./bin/dev",
    "print:help": "./bin/dev --help",
    "print:version": "./bin/dev --version",
    "update-readme": "ts-node ./src/scripts/update-readme.ts",
    "lint": "eslint --ext=.ts --config=.eslintrc --ignore-path=../../.prettierignore ."
  },
  "dependencies": {
    "@oclif/core": "^2.8.2",
    "@oclif/plugin-help": "^5.2.9",
    "chalk": "^4.1.2",
    "endent": "^2.1.0",
    "execa": "^5.1.1",
    "inquirer": "^9.1.5",
    "rimraf": "^5.0.0",
    "tslib": "^2.5.0",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.15.13",
    "@types/uuid": "^9.0.1",
    "@types/validate-npm-package-name": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "next": "^13.3.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0"
  }
}
